            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Component, EventEmitter, Input, Output } from '@angular/core';\nimport { CommonModule } from '@angular/common';\n\n@Component({\n  selector: 'lib-afi-button',\n  standalone: true,\n  imports: [CommonModule],\n  templateUrl: './button.component.html',\n  styleUrls: ['./button.component.scss'],\n})\nexport class ButtonComponent {\n  /**\n   * Is this the principal call to action on the page?\n   */\n  @Input()\n  primary = false;\n\n  /**\n   * What background color to use\n   */\n  @Input()\n  backgroundColor?: string;\n\n  /**\n   * Button contents\n   *\n   * @required\n   */\n  @Input()\n  label = 'Button';\n\n  /**\n   * Optional click handler\n   */\n  @Output()\n  onClick = new EventEmitter<Event>();\n}\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": "button {\r\n  all: unset;\r\n  padding: 1rem;\r\n  border: solid 1px pink;\r\n  cursor: pointer;\r\n  &:hover {\r\n    background-color: pink;\r\n    border-color: black;\r\n  }\r\n\r\n  &.secondary {\r\n    background-color: green;\r\n    color: white;\r\n    border-color: black;\r\n    &:hover {\r\n      background-color: transparent;\r\n      color: black;\r\n    }\r\n  }\r\n\r\n  &.pink {\r\n    background: #ff00a2;\r\n    box-shadow: 0 4px 10px rgba(55, 55, 55, 0.3),\r\n      0 6px 35px rgb(200 55 150 / 70%);\r\n  }\r\n}\r\n",
                    "styleUrl": "./button.component.scss"
                }
            ],
            "stylesData": "",
            "templateData": "<button\n  type=\"button\"\n  [ngClass]=\"{ primary: primary }\"\n  [ngClass]=\"{ secondary: !primary }\"\n  [ngStyle]=\"{ 'background-color': backgroundColor }\"\n>\n  Click Me!\n</button>\n"
        }
